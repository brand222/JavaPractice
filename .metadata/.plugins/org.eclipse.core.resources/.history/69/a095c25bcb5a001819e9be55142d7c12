package inheritance;
//tuna is extending from potpie, which also means it is inheriting from food too
public class tuna extends potpie {
	private String name;
	
	public tuna(String value) {
		
	}
	
	public String getName() {
		return name;
	}
	
	public String setName(String Name) {
		Name = name;
		return Name;
	}
	
	public void eat() {
		System.out.printf("%s\n", getName());
	}
}
