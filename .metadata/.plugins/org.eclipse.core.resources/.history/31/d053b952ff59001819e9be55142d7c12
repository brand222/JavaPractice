		//System.out.printf("%s\n", clockObject.toMilitary());
		//System.out.printf("%s\n", clockObject2.toMilitary());
		//System.out.printf("%s\n", clockObject3.toMilitary());
		System.out.printf("%s\n", clockObject4.toMilitary());
	}
}
