package basics;

public class mathClassMethods {

	
	public static void main(String[] args) {
		//absolute value example
		System.out.println(Math.abs(-26.7));
		//math.ceil will always round the number up
		System.out.println(Math.ceil(4.2));
		//math.floor will always round the number down
		System.out.println(Math.floor(2.9));
		//math.max will take two numbers and give the maximum of the two numbers
		System.out.println(Math.max(2.9, 5.3));
		//math.min will take two numbers and give the smaller of the two numbers
		System.out.println(Math.min(2.3, 2.4));
		//math.pow will take two numbers {actualnumber, ^powerNumber}
	}
}
