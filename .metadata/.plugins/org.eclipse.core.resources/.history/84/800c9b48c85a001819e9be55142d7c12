package finalExample;

public class tuna {

	private int sum;
	//this number stays the same after assigning it a value
	private final int NUMBER;
	
	public tuna(int x) {
		NUMBER = x;
	}
	
	public void add() {
		sum+=NUMBER;
	}
	public String toString() {
		return String.format("sum = %d\n", sum);
	}
}
